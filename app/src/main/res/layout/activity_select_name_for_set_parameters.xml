<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".mytrainingquickplayversion.SelectNameForSetParametersActivity">
    <LinearLayout
            android:id="@+id/mainLinearLayoutSelectNameForSetParameters"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
        <TextView
                android:id="@+id/textViewSelectNameForSetParameters"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_select_name_for_set_parameters"
                android:textSize="14pt"/>
        <Button
                android:id="@+id/buttonGoNext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_go_next"/>
        <ScrollView
                android:id="@+id/scroll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:scrollbars="none">
            <LinearLayout
                    android:id="@+id/fillableLinearLayoutSelectNameForSetParameters"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"/>
        </ScrollView>


    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>