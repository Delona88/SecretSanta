<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".quickplay.NamesOfParticipants">
    <LinearLayout
            android:id="@+id/mainLinearLayoutNamesOfParticipants"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
        <TextView
                android:id="@+id/textViewNamesOfParticipants"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/title_names_of_participants"
                android:textSize="14pt"/>
        <Button
                android:id="@+id/buttonGoToSetParameters"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Продолжить"/>
        <ScrollView
                android:id="@+id/scroll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:scrollbars="none">
            <LinearLayout
                    android:id="@+id/fillableLinearLayoutNamesOfParticipants"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"/>

        </ScrollView>
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>