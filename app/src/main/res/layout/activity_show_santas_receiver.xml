<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".quickplay.ShowSantasReceiver">
    <LinearLayout
            android:id="@+id/mainLinearLayoutShowSantasReceiver"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

        <TextView
                android:id="@+id/textViewShowSantasReceiver"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="1"
                android:textSize="14pt"/>

        <LinearLayout
                android:id="@+id/fillableLinearLayoutActivityMain6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"/>

        <Button
                android:id="@+id/buttonGoBackToSelectNameForShowReceiver"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Назад"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>